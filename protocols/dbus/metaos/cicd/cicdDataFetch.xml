<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
        "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>

    <interface name="org.metaos.CiCd.Interface">

        <method name="PipelineStatusFetch">
            <arg direction="out" type="s" name="pipelineStatus"/>
        </method>

        <signal name="statusChanging">
            <arg type="s" name="statusType"/>
        </signal>

    </interface>
</node>